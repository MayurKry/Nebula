PORT=5000
DATABASE_URL=mongodb://localhost:27017/yourdbname
NODE_ENV=development
GEMINI_API_KEY=your_gemini_api_key_here
ACCESS_SECRET=youraccesssecret
REFRESH_TOKEN_SECRET=yourrefreshtokensecret

# Development Mode - Set to true to bypass authentication for testing
BYPASS_AUTH=true

# AI Image Generation APIs (Optional - will use free APIs if not provided)
HUGGINGFACE_API_KEY=your-huggingface-api-key-here
SEGMIND_API_KEY=your-segmind-api-key-here
REPLICATE_API_KEY=your-replicate-api-key-here

# AI Provider Priority (comma-separated, default: gemini)
# Set to "gemini" to use only Gemini, or add fallbacks like "gemini,pollinations"
AI_PROVIDER_PRIORITY=gemini